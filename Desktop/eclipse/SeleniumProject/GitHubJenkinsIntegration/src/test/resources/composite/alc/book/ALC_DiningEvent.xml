<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="classes" thread-count="5">
	<listeners>
		<listener class-name="com.disney.composite.core.Screenshot"></listener>
	</listeners>
	<parameter name="browserUnderTest" value="IExplorer" />
	<parameter name="environment" value="grumpy" />
	<!-- environment=Grumpy -->
	<parameter name="runLocation" value="local" />
	<!-- runLocation=local , other option = Remote -->
	<parameter name="architecture" value="32" />
	<test name="diningEvent">
		<classes>
			<class name="com.disney.test.composite.alc.book.ALC_DiningEvent" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->